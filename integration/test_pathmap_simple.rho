// Simple PathMap Par test for MeTTa integration
// Just verifies that compile returns a Par and it's printable

new stdoutAck(`rho:io:stdoutAck`),
    mettaCompile(`rho:metta:compile`),
    ack in {

  stdoutAck!("Testing MeTTa compile with PathMap Par using !? operator...\n", *ack) |

  for (_ <- ack) {
    for (@par <- mettaCompile!?("(+ 1 2)")) {
      stdoutAck!("Success! Compiled state: ", *ack) |
      for (_ <- ack) {
        stdoutAck!(par, *ack) |
        for (_ <- ack) {
          stdoutAck!("\nTest complete!\n", *ack)
        }
      }
    }
  }
}
